extends layouts/_account
block account-list
  h4 消费明显查询
  include layouts/_search
  .table-list
    table(class="table-a",id="table-a")
      thead
        tr
          th(width="20%") 订单号
          th(width="20%") 日期
          th(width="25%") 消费品
          th(width="15%") 消费金额
          th 备注
      tbody
        - for (var i=0;i<10;i++)
          tr
            td.txt-left GPN20150410165221569081
            td 2015-04-10 16:53:45
            td.txt-left
              p(title="大皇帝(双线1区)，游戏币100...").text-overflow 大皇帝(双线1区)，游戏币100...
            td.txt-right 1000.00
            td.txt-left 顺网游戏
    include layouts/_page



